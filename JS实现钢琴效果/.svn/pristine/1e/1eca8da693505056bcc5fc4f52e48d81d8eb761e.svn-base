  data() {
    return {
      delType: 'info',
      tableheight:$(document).height()-282+'px',
      delChange: true,
      queryMonth: '2018-09',
      pickerOptions: {
        disabledDate(time) {
          return time.getTime() > Date.now()
        }
      },
      // listLoading: true,
      listLoading: false,
      importShow: false,
      detailShow: false,
      importForm: {},
      tableData3: [{
          project: '2016-05-03',
          information: '王小虎',
          unit: '上海市普陀区金沙江路 1518 弄'
        },  ]
    }
  },
  computed: {
    /* 根据table数据的总页数，来展示 */
    realPageSizes() {
      const pageSizes = [10, 20, 30, 40]
      for (let i = 0; i < pageSizes.length; i++) {
        if (this.tablePageTotal < pageSizes[i]) {
          return pageSizes.slice(0, i + 1)
        }
      }
    },
    ...mapState({
      list: state => state.xnyRep.list,
      currentPage: state => state.xnyRep.currentPage,
      totalCount: state => state.xnyRep.totalCount,
      detail: state => state.xnyRep.detail
    })
  },
  created() {
    this.fetchData(1)
  },
  methods: {
    RuleRequired(message) {
      return [{ required: true, message }]
    },
    fetchData(currPage, pageSize = 10) {
      console.log('1111')
      this.$store.dispatch('GetList_rxny', {
        cb: () => {

        }
      })
      this.listLoading = false
    },
    triggerDate() {
      this.$refs.elDatePick.focus()
      this.$refs.triggerDate.blur()
    },
    updateSizeChange(val) {
      this.fetchData(this.currentPage, val)
    },
    updateCurrentChange(val) {
      this.fetchData(val)
    },
    handleSelectionChange(val) {
      if (val.length > 0) {
        this.delType = 'primary'
        this.delChange = false
        this.multipleSelection = val
      } else if (val.length === 0) {
        this.delType = 'info'
        this.delChange = true
      }
    },
    addRep() {
      this.$store.dispatch('Add_rxny', {
        cb: () => {}
      })
      this.$message({ type: 'success', message: '成功' })
    },
    importRep() {
      this.importShow = true
      this.importForm.year = ''
      this.importForm.pictureFile = ''
    },
    showDetail(row) {
      this.detailShow = true
      // this.$store.dispatch('GetDetail_rmonth', {
      //   id: row.id,
      //   cb: () => {
      //     this.detailShow = true
      //   }
      // })
    },
    hidePopup() {
      this.importShow = false
      this.detailShow = false
    },
    repDownload(row) {
      const url = 'www.baidu.com'
      window.open(url)
    },
    deleteRep(index) {
      this.$confirm('此操作将永久删除该数据, 是否继续', '提示', {
        confirmButtonText: '确定',
        cancelButtonText: '取消',
        type: 'warning'
      }).then(() => {
				this.$store.dispatch('DeleteData_rxny',{
					id:index.$index,
					cb: ()=>{
						this.$message({ type: 'success', message: '删除成功' })
					}
				})
        
      })
    },
    repBatchDel() {
      this.$confirm('确定要删除所选文件吗，此操作不可逆', '提示', {
        confirmButtonText: '确定',
        cancelButtonText: '取消',
        type: 'warning'
      }).then(() => {
        this.$message({ type: 'success', message: '删除成功' })
      })
    }
  }
}
</script>
